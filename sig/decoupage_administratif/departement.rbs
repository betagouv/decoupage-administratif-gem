module DecoupageAdministratif
  class Departement
    extend BaseModel
    attr_reader code: String
    attr_reader nom: String
    attr_reader zone: String
    attr_reader region_code: String

    def initialize: (
        code: String,
        nom: String,
        zone: String,
        region_code: String
      ) -> void


    def self.all: () -> Array[Departement]

    def communes: () -> Array[Commune]

    def region: () -> Region
  end
end
